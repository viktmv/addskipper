{

  "manifest_version": 2,
  "name": "Addskipper",
  "version": "1.0",

  "description": "Rewinds videos to the end (including adds)",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["addskipper.js"]
    }
  ],

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/temp-32.png",
    "default_title": "Activate",
    "default_popup": "popup/popup.html"
  }
}
