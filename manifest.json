{

  "manifest_version": 2,
  "name": "Addskipper",
  "version": "1.0",

  "description": "Skips Youtube Adds with the click of a button.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["addskipper.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/temp-32.png",
    "default_title": "Beastify",
    "default_popup": "popup/choose_beast.html"
  }
}
